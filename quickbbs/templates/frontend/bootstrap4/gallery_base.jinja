            <!-- Dropdown menu -->
                    <li class="nav-item dropdown">
            <!--             User Name -->
                    <a class="nav-link dropdown-toggle btn-lg" id="navbarDropdownMenuLink" data-toggle="dropdown">
                        {% if user.username == '' %}
                              {{ icon("users fa-2x")}}
                        {% else %}
                              {{ user.username }}
                        {% endif %}
                        <span class="caret"></span></a>

                    <div class="dropdown-menu  dropdown-menu-right" aria-labelledby="navbarDropdownMenuLink">
